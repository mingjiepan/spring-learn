<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!--id 和 name必须是唯一的，不能和其他bean的id或者name重复，同一个bean的name可以有多个名称，名称可以重复-->
    <bean id="userDao" name="myUserDao;myUserDao2" class="com.mjie.xml.beans.name.dao.UserDao"/>

    <bean id="peopleDao" name="peopleDao2" class="com.mjie.xml.beans.name.dao.PeopleDao"/>
</beans>